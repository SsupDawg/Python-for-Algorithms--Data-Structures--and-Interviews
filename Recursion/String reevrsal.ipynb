   "source": [
    "\n",
    "# Reverse a String\n",
    "This interview question requires you to reverse a string using recursion. Make sure to think of the base case here."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {
    "collapsed": false
   },
   "outputs": [
    {
     "data": {
      "text/plain": [
       "'nashe'"
      ]
     },
     "execution_count": 2,
     "metadata": {
     },
     "output_type": "execute_result"
    }
   ],
   "source": [
    "def reverse(str):\n",
    "    if len(str)==1:\n",
    "        return str\n",
    "    else:\n",
    "        return str[len(str)-1]+reverse(str[0:len(str)-1])\n",
    "reverse('ehsan')"
   ]
